<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.andikasoft.rwandacattle.rwandacattle">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:name=".ChangeLanguage"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BreederLoginActivity"
            android:parentActivityName="com.andikasoft.rwandacattle.rwandacattle.MainActivity"></activity>
        <activity
            android:name=".BreederRegisterActivity"
            android:parentActivityName="com.andikasoft.rwandacattle.rwandacattle.BreederLoginActivity"></activity>
        <activity
            android:name=".VetLoginActivity"
            android:parentActivityName="com.andikasoft.rwandacattle.rwandacattle.MainActivity"></activity>
        <activity
            android:name=".VetRegisterActivity"
            android:parentActivityName="com.andikasoft.rwandacattle.rwandacattle.VetLoginActivity"></activity>
        <activity
            android:name=".HelpActivity"
            android:parentActivityName=".MainActivity"></activity>
        <activity android:name=".VetHomeActivity"></activity>
        <activity android:name=".BreederHomeActivity"></activity>

    </application>

</manifest>